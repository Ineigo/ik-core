{
  "name": "ik-core",
  "author": "Ineigo Kira",
  "version": "1.1.1",
  "description": "Some simple instruments for Game Dev",
  "declaration": true,
  "keywords": [
    "engine",
    "game dev",
    "game development"
  ],
  "prepublish": "tsc && bunx --bun vite build",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ineigo/ik-core.git"
  },
  "license": "GNU",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "tsc && bunx --bun vite build",
    "format": "rome format ./src --write",
    "tsc": "tsc"
  },
  "dependencies": {
    "tsc": "^2.0.4"
  },
  "devDependencies": {
    "rome": "12.1.3-nightly.4c8cf32",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/node": "^20.6.5",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "tslib": "^2.6.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}